{
  "name": "8x8-developer-docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "lowercase-operationids": "node scripts/lowercase-operation-ids.js",
    "reference:clean": "find docs -type d -name 'reference' -exec rm -rf {} +",
    "reference:generate": "docusaurus gen-api-docs all",
    "reference": "yarn reference:clean && yarn reference:generate",
    "validate:sidebar": "node scripts/validate-sidebar.js",
    "validate:content": "node scripts/validate-content.js",
    "validate:oas": "redocly lint \"docs_oas/**/*.{json,yaml,yml}\"",
    "validate:openapi": "node scripts/validate-openapi.js",
    "analyze-links": "node scripts/analyze-broken-links.js",
    "format:check": "prettier --check \"**/*.{js,jsx,json}\"",
    "format:write": "prettier --write \"**/*.{js,jsx,json}\""
  },
  "dependencies": {
    "@docusaurus/core": "^3.9.1",
    "@docusaurus/plugin-client-redirects": "^3.9.1",
    "@docusaurus/plugin-content-docs": "^3.9.1",
    "@docusaurus/plugin-google-gtag": "^3.9.1",
    "@docusaurus/preset-classic": "^3.9.1",
    "@docusaurus/theme-live-codeblock": "^3.9.1",
    "@easyops-cn/docusaurus-search-local": "^0.52.1",
    "@mdx-js/react": "^3.1.1",
    "clsx": "^2.1.1",
    "docusaurus-plugin-openapi-docs": "^4.5.1",
    "docusaurus-theme-openapi-docs": "^4.5.1",
    "prism-react-renderer": "^2.4.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "sass": "1.93.2"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^3.9.1",
    "@redocly/cli": "^2.7.0",
    "prettier": "^3.6.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=16.14"
  },
  "prettier": {
    "arrowParens": "avoid",
    "singleQuote": true,
    "trailingComma": "all"
  },
  "packageManager": "yarn@4.2.2"
}
